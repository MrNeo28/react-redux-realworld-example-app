{
    "compilerOptions": {
      "baseUrl": "./",
      "module": "commonjs",
      "lib": [
        "dom",
        "es6"
      ],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "types": ["cypress", "node", "@testing-library/cypress"],
      "noEmit": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
  
